{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "A mod which substantially improves Minecraft's data driven world generation error detection and recovery mechanisms.",
  "authors": [
    "${modAuthor}"
  ],
  "contact": {
    "homepage": "${modHomeUrl}",
    "sources": "${modSourceUrl}",
    "issues": "${modIssueUrl}"
  },
  "license": "MIT",
  "icon": "assets/${modId}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [],
    "client": []
  },
  "mixins": [
    "cyanide.mixins.json",
    "cyanide.common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricLoaderVersion}",
    "minecraft": ">=${minecraftVersion}",
    "java": ">=${modJavaVersion}"
  },
  "accessWidener": "cyanide.accesswidener"
}
  